FOLDERS = env.FOLDERS
DESTINATION = env.DESTINATION
SOURCE = env.SOURCE

node {
    stage('Install dependencies') {
        echo 'Installing dependencies'
        sh 'sudo apt -y install rsync'
    }

    for(String folder : FOLDERS.split(",")) {
        stage("Synch ${folder}") {
           echo "rsync -avn ${SOURCE}:${folder} ${DESTINATION}"
    }
    }
}